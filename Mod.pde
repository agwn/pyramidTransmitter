class ModFloat {
  private float value;

  ModFloat(float value_) {
    value = value_;
  }

  void set(float value_) {
    value = value_;
  }

  float get() {
    return value;
  }
}

class ModColor {
  private color c;

  ModColor(color c_) {
    c = c_;
  }

  void set(color c_) {
    c = c_;
  }

  color get() {
    return c;
  }
}
